{% for flash_message_type, flash_messages in app.session.flashbag.all() %}
  {% for flash_message in flash_messages %}
    {% if flash_message_type == 'error' %}
      <div class="alert alert-danger alert-dismissible fade show alert-small" role="alert">
        {{ flash_message }}
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    {% elseif flash_message_type == 'success' %}
      <div class="alert alert-success alert-dismissible fade show alert-small" role="alert">
        {{ flash_message }}
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    {% endif %}
  {% endfor %}
{% endfor %}

